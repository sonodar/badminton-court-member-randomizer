{
  "name": "@doubles-member-generator/event-cleaner",
  "version": "2.0.0",
  "description": "Lambda function generated by Amplify",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -f src/*.js src/*.map src/*.d.ts",
    "compile": "esbuild src/index.ts --outfile=src/index.js --bundle --minify --sourcemap=external --platform=node --target=node18 '--external:@aws-sdk/client-*' '--external:@aws-sdk/lib-*'",
    "build": "run-s clean compile",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{ts,json}'",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.450.0",
    "@aws-sdk/lib-dynamodb": "3.450.0",
    "@types/aws-lambda": "8.10.92",
    "esbuild": "0.19.5"
  }
}
