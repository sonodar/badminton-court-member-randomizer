{
  "version": 1,
  "routes": [
    {
      "path": "/_astro/*",
      "target": {
        "kind": "Static",
        "cacheControl": "public, max-age=31536000, immutable"
      }
    },
    {
      "path": "/*.*",
      "target": {
        "kind": "Static"
      },
      "fallback": {
        "kind": "Compute",
        "src": "default"
      }
    },
    {
      "path": "/*",
      "target": {
        "kind": "Compute",
        "src": "default"
      }
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "entrypoint": "entry.mjs",
      "runtime": "nodejs20.x"
    }
  ],
  "framework": {
    "name": "astro",
    "version": "4.3.2"
  }
}
