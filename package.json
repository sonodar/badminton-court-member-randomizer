{
  "name": "doubles-member-generator",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "rm -rf dist; astro build",
    "bundle": "esbuild --banner:js='import {createRequire} from \"module\";const require = createRequire(import.meta.url);' --bundle --platform=node --target=node20 --outfile=dist/server/index.mjs --external:sharp --format=esm dist/server/entry.mjs",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "biome lint .",
    "lint:fix": "biome lint --apply .",
    "test": "vitest run",
    "format": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "fix": "run-s format lint:fix typecheck format",
    "amplify:DoublesMemberGeneratorEventCleaner": "yarn install; yarn --cwd amplify/backend/function/DoublesMemberGeneratorEventCleaner run build"
  },
  "dependencies": {
    "@astrojs/node": "^8.2.0",
    "@astrojs/react": "3.0.9",
    "@chakra-ui/icons": "2.1.1",
    "@chakra-ui/react": "2.8.2",
    "@dnd-kit/core": "^6.1.0",
    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@types/ms": "0.7.34",
    "@types/react": "18.2.53",
    "@types/react-dom": "18.2.18",
    "astro": "^4.3.2",
    "aws-amplify": "6.0.13",
    "esbuild": "^0.20.0",
    "framer-motion": "11.0.3",
    "jotai": "2.6.4",
    "ms": "2.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.0.1",
    "ts-node": "10.9.2",
    "ts-pattern": "5.0.6",
    "typescript": "5.3.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@vitest/ui": "1.2.2",
    "npm-run-all": "4.1.5",
    "vitest": "1.2.2"
  },
  "workspaces": [
    "amplify/backend/function/*"
  ]
}
