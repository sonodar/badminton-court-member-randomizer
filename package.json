    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-unused-imports": "3.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "ts-node": "10.9.2",
    "vitest": "1.2.2"
