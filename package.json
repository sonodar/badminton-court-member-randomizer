    "@emotion/react": "11.11.3",
    "@emotion/styled": "11.11.0",
    "@types/ms": "0.7.34",
    "@types/react": "18.2.53",
    "@types/react-dom": "18.2.18",
    "astro": "^4.3.2",
    "aws-amplify": "6.0.13",
    "framer-motion": "11.0.3",
    "jotai": "2.6.4",
    "ms": "2.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
